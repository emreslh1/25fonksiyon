#include <stdio.h>

float bolum(float sayi1 , float sayi2)
{
    return sayi1/sayi2;
}
int main(){
 float sayi1,sayi2;
  printf("ilk sayiyi giriniz:");
  scanf("%f",&sayi1);

  printf("ikinci sayiyi giriniz:");
  scanf("%f",&sayi2);

  printf("sayilarin bolumuu = %f",bolum(sayi1,sayi2));





    return 0;
}
